{
    "HeatStacks.create_and_delete_stack": [
        {
            "args": {
               "template_path": {{resource_group_server_with_volume}},
               "parameters": {
                   "num_instances": 2
               },
               "files": [{{server_with_volume}}]
            },
            "runner": {
                "type": "constant",
                "times": 3,
                "concurrency": 1
            },
            "context": {
                "users": {
                    "tenants": 2,
                    "users_per_tenant": 3
                }
            },
            "sla": {
                "failure_rate": {
                    "max": 0
                }
            }
        }
    ]
}
